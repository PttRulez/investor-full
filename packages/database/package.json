{
  "name": "database",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./index.ts",
  "scripts": {
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push --skip-generate",
    "db:migrate": "npx prisma migrate dev",
    "db:prod": "npx prisma generate && yes | npx prisma migrate deploy",
    "db:reset": "npx prisma migrate reset",
    "build": "tsc"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1"
  },
  "devDependencies": {
    "prisma": "^5.3.1"
  },
  "prisma": {
    "seed": "ts-node ./prisma/seed.ts"
  }
}
